{
    "name": "iter8/adldap2-service-provider",
    "description": "Service Provider for the adLDAP PHP library.",
    "version": "3.0.1",
    "license": "MIT",
    "type": "library",
    "config": {
        "preferred-install": "dist",
        "platform": {
            "php": "7.1.8"
        },
        "sort-packages": true
    },
    "require": {
        "php": ">=7.1",
        "ext-ldap": "*",
        "adldap2/adldap2": "~5.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "~7.0",
        "silex/silex": "~2.0"
    },
    "autoload": {
        "psr-4": {
            "Adldap2ServiceProvider\\": "src/Adldap2ServiceProvider"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests"
        }
    },
    "scripts": {
        "tests": "phpunit"
    }
}
